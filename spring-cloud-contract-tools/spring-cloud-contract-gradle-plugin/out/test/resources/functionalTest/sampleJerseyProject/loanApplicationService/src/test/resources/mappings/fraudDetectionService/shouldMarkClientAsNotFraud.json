{
	"request": {
		"method": "PUT",
		"headers": {
			"Content-Type": {
				"equalTo": "application/vnd.fraud.v1+json"
			}
		},
		"url": "/fraudcheck",
		"bodyPatterns": [
			{
				"matches": "\\s*\\{\\s*\"clientPesel\"\\s*:\\s*\"?[0-9]{10}\"?\\s*,\\s*\"loanAmount\"\\s*:\\s*\"?123.123\"?\\s*\\}\\s*"
			}
		]
	},
	"response": {
		"status": 200,
		"headers": {
			"Content-Type": "application/vnd.fraud.v1+json"
		},
		"body": "{\"fraudCheckStatus\":\"OK\",\"rejectionReason\":null}"
	}
}