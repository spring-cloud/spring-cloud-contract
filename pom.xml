							<include>**/*Tests.*</include>
							<include>**/*Test.*</include>
						</includes>
					</configuration>
				</plugin>
				<plugin>
					<groupId>org.codehaus.plexus</groupId>
					<artifactId>plexus-component-metadata</artifactId>
					<version>1.6</version>
				</plugin>
				<plugin>
